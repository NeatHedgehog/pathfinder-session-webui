@model List<AutoMailerWebUI.Models.ResponseList>

@{
    ViewData["Title"] = "RSVP List";
}

@if(Model.Count > 0)
{
    foreach (AutoMailerWebUI.Models.ResponseList rl in Model)
    {
        Html.RenderPartial("_SubRSVPList", rl);
    }
}

else
{
    <header>
        <h2>No current sessions</h2>
    </header>
}